{
  "mcpServers": {
    "shadcn-components": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@jpisnice/shadcn-ui-mcp-server",
        "--github-api-key",
        "${GITHUB_API_KEY}"
      ],
      "env": {}
    },
    "shadcn-themes": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "shadcn@canary",
        "registry:mcp"
      ],
      "env": {
        "REGISTRY_URL": "https://tweakcn.com/r/themes/registry.json"
      }
    }
  }
}
