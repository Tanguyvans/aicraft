{
  "name": "aicraft",
  "version": "0.3.0",
  "description": "AI Agent Package Manager for Claude - Install and manage AI agents",
  "keywords": [
    "ai",
    "claude",
    "agents",
    "cli",
    "typescript"
  ],
  "homepage": "https://github.com/Tanguyvans/aicraft",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tanguyvans/aicraft.git"
  },
  "license": "MIT",
  "author": "Tanguy Vansnick",
  "type": "module",
  "bin": {
    "aicraft": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "agents"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.1",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.9.4",
    "ora": "^8.2.0"
  }
}
